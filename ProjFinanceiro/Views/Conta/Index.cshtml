
@{
    ViewData["Title"] = "Index";
}

<h2>Minhas contas</h2>

<table class="table-bordered">
    <THEAD>
        <tr>
            <th>#</th>
            <th>ID</th>
            <th>Nome</th>
            <th>Saldo</th>
        </tr>
    </THEAD>

    @{ 
        foreach(var item in (List<ContaModel>)ViewBag.ListaConta){
            <tbody>
                <tr>
                    <td><button class="btn btn-danger" type="button">Excluir</td>
                    <td>@item.Id.ToString()</td>
                    <td>@item.Nome.ToString()</td>
                    <td>@item.Saldo.ToString()</td>
                </tr>
            </tbody>
        }
    }

</table>

